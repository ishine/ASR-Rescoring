def rescore(weight, hyps_len, am, lm):
    am = np.array(am)
    lm = np.array(lm)
    hyps_len = np.array(hyps_len)
    final_score = (1-weight)*(am)/hyps_len + weight*(lm)/hyps_len
    #final_score = (1-weight)*(am) + weight*(lm)
    return final_score

